# keys.env.example - Copy to keys.env and fill in values

# Finnhub API
FINNHUB_API_KEY=your_finnhub_api_key_here

# Alpaca API (Paper Trading)
ALPACA_API_KEY=your_alpaca_api_key_here
ALPACA_SECRET_KEY=your_alpaca_secret_key_here
ALPACA_BASE_URL=https://paper-api.alpaca.markets

# Alternative Alpaca env var names (for alpaca-py compatibility)
APCA_API_KEY_ID=your_alpaca_api_key_here
APCA_API_SECRET_KEY=your_alpaca_secret_key_here

# Database
DATABASE_URL=sqlite+aiosqlite:///./data/bars.db
# For PostgreSQL:
# DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/financial_dashboard

# Server Configuration
API_HOST=0.0.0.0
API_PORT=8000
LOG_LEVEL=INFO

# Cache Settings
CACHE_MAX_SIZE=10000
DEDUP_WINDOW_SIZE=10000

# Bar Engine
BAR_TIMEFRAMES=1m,5m,15m,1h,1d
INCLUDE_EXTENDED_HOURS=false
